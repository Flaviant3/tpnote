{# templates/video/index.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Liste des Vidéos{% endblock %}

{% block body %}
    <h1>Liste des Vidéos</h1>

    <ul>
        {% for video in videos %}
            <li>
                <a href="{{ path('video_show', {'id': video.id}) }}">{{ video.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
